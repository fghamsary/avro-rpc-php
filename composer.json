{
  "name": "v3d/avro-rpc-php",
  "description": "An Avro RPC client in PHP (compatible with the Avro RPC server in Java)",
  "keywords": [
    "avro",
    "rpc",
    "serialization"
  ],
  "license": "proprietary",
  "version": "1.8.3",
  "type": "library",
  "homepage": "https://gitlab.v3d.fr/server/avro-rpc-php",
  "authors": [
    {
      "name": "V3D Server Team",
      "email": "dev_srv@v3d.fr"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://ovh-repo-01.v3d.fr/repository/v3d-php"
    }
  ],
  "require": {
    "ext-json": "*",
    "ext-gmp": "*",
    "ext-sockets": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "7.*"
  },
  "suggest": {
    "v3d/avro-rpc-php-generator": "Generates an Avro RPC client as a PHP class based on your Avro protocol"
  },
  "autoload": {
    "psr-4": {
      "Avro\\": "src/Avro/"
    }
  }
}